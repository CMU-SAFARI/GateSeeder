set terminal svg size 800, 600 enhanced font 'FreeSans,18'
set output "distribution_80.svg"
n = 100
max = 3000
min = 0
width = (max - min)/n
hist(x, width) = width*floor(x/width)+width/2.0
set xlabel "Number of queried locations belonging to the golden location range"
set boxwidth width
set style fill solid
plot "distribution_80.dat" u (hist($1,width)):(1.0) smooth freq w boxes lc rgb"blue" notitle
